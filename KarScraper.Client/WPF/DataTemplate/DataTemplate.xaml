<ResourceDictionary x:Name="Datatemplate"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:localVM="clr-namespace:KarScraper.ViewModel"        
        xmlns:local="clr-namespace:KarScraper">

    <DataTemplate DataType="{x:Type localVM:Page1ViewModel}">
        <local:Page1View/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type localVM:Page2ViewModel}">
        <local:Page2View/>
    </DataTemplate>


</ResourceDictionary>